<Window x:Class="Shope.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shope"
        mc:Ignorable="d"
        Title="HardWare" Height="550" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="27*"/>
            <RowDefinition Height="183*"/>
        </Grid.RowDefinitions>
        <Image Grid.Row="0" x:Name="LogoImage" HorizontalAlignment="Center" VerticalAlignment="Center"></Image>
        <Frame x:Name="MainFrame" Grid.Row="1"></Frame>
    </Grid>
</Window>
